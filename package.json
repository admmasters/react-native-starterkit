{
	"name": "StarterKit",
	"version": "0.0.1",
	"private": true,
	"scripts": {
    "open:ios": "react-native run-ios",
    "start": "npm-run-all --parallel start-packager open:ios lint:watch test:watch",
    "start-packager": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "lint": "esw",
    "lint:watch": "npm run lint -- --watch",
    "typecheck": "flow"
	},
  "dependencies": {
    "babel-preset-react-native": "^1.9.1",
    "react": "16.0.0-alpha.6",
    "react-native": "0.43.3",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "redux-immutable-state-invariant": "^2.0.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-jest": "19.0.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-plugin-killua": "^1.0.1",
    "eslint-watch": "^3.1.0",
    "flow-bin": "^0.40.0",
    "jest": "19.0.2",
    "jest-enzyme": "^3.0.1",
    "npm-run-all": "^4.0.2",
    "react-native-mock": "^0.3.1",
    "react-test-renderer": "16.0.0-alpha.6"
  },
  "jest": {
    "preset": "react-native",
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js"
  },
  "eslintConfig": {
    "extends": "plugin:killua/standard"
  },
  "babel": {
    "presets": ["react-native"]
  }
}